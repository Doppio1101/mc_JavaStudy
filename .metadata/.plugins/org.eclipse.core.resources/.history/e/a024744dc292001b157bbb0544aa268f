package mc.sn.day4;

public class Studio {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Studio s = new Studio();
		s.readyLottoBall();
	}
	
	public void readyLottoBall() {
		LottoBall[] ballBox = new LottoBall[45];
		LottoBall ball = null;
		int n =0;
		for(int i=0; i<45; i++) {
			n = i +1;
			ball = new LottoBall(n+"");
			ballBox[i] = ball;
		}
		
		
		
		System.out.println(ball.getBallNumber());
	
	
	}

}
