package mc.sn.day6;

public class CodeExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		CodeExample ce = new CodeExample();
		ce.test1(3, 8);
	}
	
	public void test1(int a, int b) {
		//교환로직 연습
		//두 개의 정수형 변수를 입력받게 
		//매개변수 리스트를 변경하시오.
		//a에서 b까지 출력한다. (a,b 포함)
		
		for(int i = a; i<b+1; i++) {
			System.out.println(i);
		}
		
		
		
	}

}
